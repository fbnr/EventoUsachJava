<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="eusach" transaction-type="JTA">
		<jta-data-source>jdbc/eusach</jta-data-source>
		<class>model.Usuario</class>
		<class>model.TipoEstado</class>
		<class>model.Tipo</class>
		<class>model.Lugar</class>
		<class>model.UsuarioSeguidos</class>
		<class>model.Preferencia</class>
		<class>model.Evento</class>
	</persistence-unit>
</persistence>
